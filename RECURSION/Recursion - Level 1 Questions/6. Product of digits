class Main {
    public static void main(String[] args) {
       int n=1234;
       System.out.print(productofdigits(n));
    }
    static int productofdigits(int n)
    {
        if(n==0)
        {
            return 1;
        }
        int rem=n%10;
        return rem*productofdigits(n/10);
    }
}
