class Main {
    public static void main(String[] args) {
       int n=1090008;
       System.out.print(countz(n));
    }
    static int c=0;
    static int countz(int n)
    {
        if(n==0)
        {
            return c;
        }
        int rem=n%10;
        if(rem==0) c++;
        return countz(n/10);
    }
}
