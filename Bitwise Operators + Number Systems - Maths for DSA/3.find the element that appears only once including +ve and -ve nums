T.C : O(1)
S.C : O(1)

class Main {
    public static void main(String[] args) {
        int[] arr = {2, -3, 4, 1, 3, -1, -2, 6, -4, 2, -3, 4, 1, 3, -1, -2, -4};
        int res = 0;
        for (int i : arr) {
            res ^= i; 
        }
        System.out.println("Single number: " + res);
    }
}
